-- ********************** INSERT DATA IN manage_enp TABLE  **********************
CREATE TABLE manage_enp(
            MANEGE_ID INT PRIMARY KEY,
            EMP_NO INT REFERENCES EMPLOYEE(EMP_NO),
             MAN_ID INT REFERENCES MANAGER(MAN_ID)  
            
            
 );

