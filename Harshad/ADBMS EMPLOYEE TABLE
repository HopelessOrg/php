-- ********************** INSERT DATA IN WORKS TABLE  **********************
CREATE TABLE WORKS(  
         WOR_ID INT PRIMARY KEY,  
         COM_ID INT REFERENCES COMPANY(COM_ID) ,  
         EMP_NO INT REFERENCES EMPLOYEE(EMP_NO),
	       SALARY INT 
  );

